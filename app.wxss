/**app.wxss**/
@import '/iconfont.wxss';
.container {
  height: 100%;
  height: 100vh;
}
.button[type='default']{
  width: 100%; 
  font-weight: normal;
  font-size: 32rpx;
  height: 80rpx;
  border-radius: 40rpx;
}
.button.hover{
  opacity: .9;
}
page{
  background-color: #F5F7FA;
}
.overhide2{
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.overhide{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.iconleft{
  font-size: 32rpx;
  color: #C0C4CC;
}